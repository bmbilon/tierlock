{
  "name": "tierlock",
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/intake.html",
      "dest": "/intake.html"
    },
    {
      "src": "/intake",
      "dest": "/intake.html"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
